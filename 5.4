class Solution(object):
    def longestPalindrome(self,s) :
        res=list(s)
        tam=len(res)
        for i in range(tam):
            temp=""
            if(''.join([str(elem) for elem in res]) == ''.join([str(elem) for elem in res]
                )[::-1]):
                    return(''.join([str(elem) for elem in res]))
                    break
                
            else:
                temp=res[len(res)-1]
                res.pop()
                if(''.join([str(elem) for elem in res]) == ''.join([str(elem) for elem in res]
                )[::-1]):
                    return(''.join([str(elem) for elem in res]))
                    break
                
                if(''.join([str(elem) for elem in res]) != ''.join([str(elem) for elem in res]
                )[::-1]):
                    res.append(temp)
                    res.pop(0)
        
        
        #return res
   
   

