from itertools import combinations
class Solution(object):
    def longestPalindrome(self,s) :
        
        return max([s[x:y] for x, y in combinations(
            range(len(s) + 1), r = 2) if  s[x: y] ==  s[x: y][::-1]], key=len)
        
        
                
   
   

w=Solution().longestPalindrome("")
print(w) 

