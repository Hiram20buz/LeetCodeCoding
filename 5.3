class Solution(object):
    def longestPalindrome(self,s) :
        res=[]
        
        for i in range(len(s)):
            if  s[i: len(s)] ==  s[i: len(s)][::-1]:
                res.append(s[i: len(s)])
                break
                
        for i in range(len(s)):
            if  s[0: len(s)-i] ==  s[0: len(s)-i][::-1]:
                res.append(s[: len(s)-i])
                break
            
        for i in range(len(s)):
            if  s[i: len(s)-i] ==  s[i: len(s)-i][::-1]:
                res.append(s[i: len(s)-i])
                break
            
        return max(res, key=len)
                
   
   

w=Solution().longestPalindrome("")
print(w) 

