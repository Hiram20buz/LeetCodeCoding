class Solution(object):
    def longestPalindrome(self,s) :
        pal = {}
        
        #print(a_dict)
        for palabra in [  s[i: j]  for i in range(len(s)) for j in range(i + 1, len(s) + 1) if  s[i: j] ==  s[i: j][::-1]]:
            pal[palabra] = len(palabra)
        #print(pal)       
        
        return max(pal, key=pal.get)
        
        
                
   
   

w=Solution().longestPalindrome("")
print(w) 

