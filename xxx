class Solution:
    def intToRoman(self, num: int) -> str:
        roman = {
          "I": 1,
          "V": 5,
          "X": 10,
          "L": 50,
          "C": 100,
          "D": 500,
          "M": 1000
        }
        print('M'*int(num/roman['M']))
        for key in reversed(roman):
            print(key)

        
        
a=Solution().intToRoman(3999)
